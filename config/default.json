{
  "appCredentials": {
    "clientId": "@@MDSP_KEY_STORE_CLIENT_ID",
    "clientSecret": "@@MDSP_KEY_STORE_CLIENT_SECRET",
    "appName": "@@MDSP_OS_VM_APP_NAME",
    "appVersion": "@@MDSP_OS_VM_APP_VERSION",
    "appId": "@@MDSP_OS_VM_APP_ID"
  },
  "appSettings": {
    "appContainerTenant": "@@HOST_TENANT_NAME",
    "appContainerAssetId": "@@APP_ASSET_ID",
    "appContainerAssetType": "sidiop.SIDIRO_APP_CONTAINER",
    "appAssetType": "sidiop.SIDIRO_APP",
    "subtenantAssetType": "core.basicsubtenant"
  },
  "userPermissions": {
    "superAdminRole": "sidiop.super_admin",
    "globalAdminRole": "mdsp_customrole:testGlobalAdmin",
    "globalUserRole": "mdsp_customrole:testGlobalUser",
    "localAdminRole": "mdsp_customrole:testLocalAdmin",
    "localUserRole": "mdsp_customrole:testLocalUser",
    "msStandardUser": "mdsp:core:StandardUser",
    "msSubtenantUser": "mdsp:core:SubTenantUser"
  },
  "port": 5000,
  "tokenExpireAdditionalTime": 100000,
  "logging": {
    "info": {
      "path": "logs/info.log",
      "maxsize": 10485760,
      "maxFiles": 1
    },
    "warning": {
      "path": "logs/warn.log",
      "maxsize": 10485760,
      "maxFiles": 1
    },
    "error": {
      "path": "logs/error.log",
      "maxsize": 10485760,
      "maxFiles": 1
    }
  }
}
