{
  "appCredentials": {
    "clientId": "@@MDSP_KEY_STORE_CLIENT_ID",
    "clientSecret": "@@MDSP_KEY_STORE_CLIENT_SECRET",
    "appName": "@@MDSP_OS_VM_APP_NAME",
    "appVersion": "@@MDSP_OS_VM_APP_VERSION",
    "appId": "@@MDSP_OS_VM_APP_ID",
    "hostTenant": "@@HOST_TENANT_NAME"
  },
  "appSettings": {
    "appContainerTenant": "@@APP_CONTAINER_TENANT_NAME",
    "appContainerAssetId": "@@APP_ASSET_ID",
    "appContainerAssetType": "@@APP_CONTAINER_ASSET_TYPE",
    "appAssetType": "@@APP_ASSET_TYPE",
    "serviceContainerAssetId": "@@APP_SERVICE_CONTAINER_ASSET_ID"
  },
  "userPermissions": {
    "globalAdminScope": "@@GLOBAL_ADMIN_SCOPE",
    "globalUserScope": "@@GLOBAL_USER_SCOPE",
    "localAdminScope": "@@LOCAL_ADMIN_SCOPE",
    "localUserScope": "@@LOCAL_USER_SCOPE",
    "globalAdminGroup": "@@GLOBAL_ADMIN_GROUP",
    "globalUserGroup": "@@GLOBAL_USER_GROUP",
    "localAdminGroup": "@@LOCAL_ADMIN_GROUP",
    "localUserGroup": "@@LOCAL_USER_GROUP",
    "msStandardUserGroup": "@@MS_STANDARD_USER_GROUP",
    "msSubtenantUserGroup": "@@MS_SUBTENANT_USER_GROUP",
    "superAdminUserIds": "@@SUPER_ADMIN_IDS"
  },
  "port": 5000,
  "tokenExpireAdditionalTime": 100000,
  "logging": {
    "info": {
      "path": "logs/info.log",
      "maxsize": 10485760,
      "maxFiles": 1
    },
    "warning": {
      "path": "logs/warn.log",
      "maxsize": 10485760,
      "maxFiles": 1
    },
    "error": {
      "path": "logs/error.log",
      "maxsize": 10485760,
      "maxFiles": 1
    }
  }
}
